{
  "manifest_version": 3,
  "name": "Eazy Captions",
  "description": "YouTube caption Enhancer",
  "version": "1.0",
  "action": {
    "default_popup": "index.html",
    "default_icon": "icon.png"
  },
  "content_scripts": [
    {
      "js": ["scripts/content.js"],
      "matches": [
        "https://www.youtube.com/watch?*"
      ]
    }
  ]
}